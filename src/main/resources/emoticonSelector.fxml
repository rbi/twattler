<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:controller="de.saxsys.twattler.EmoticonsController" prefHeight="60.0" prefWidth="90.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
<children>
<Button fx:id="smile" mnemonicParsing="false">
  <styleClass>
    <String fx:value="emoticonButton" />
    <String fx:value="smile" />
  </styleClass>
</Button>
<Button fx:id="biggrin" mnemonicParsing="false" GridPane.columnIndex="1">
  <styleClass>
    <String fx:value="emoticonButton" />
    <String fx:value="biggrin" />
  </styleClass>
</Button>
<Button fx:id="confused" mnemonicParsing="false" GridPane.columnIndex="2">
  <styleClass>
    <String fx:value="emoticonButton" />
    <String fx:value="confused" />
  </styleClass>
</Button>

<Button fx:id="mad" mnemonicParsing="false" GridPane.rowIndex="1">
  <styleClass>
    <String fx:value="emoticonButton" />
    <String fx:value="mad" />
  </styleClass>
</Button>
<Button fx:id="sad" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="1">
  <styleClass>
    <String fx:value="emoticonButton" />
    <String fx:value="sad" />
  </styleClass>
</Button>
<Button fx:id="eek" mnemonicParsing="false" GridPane.columnIndex="2" GridPane.rowIndex="1">
  <styleClass>
    <String fx:value="emoticonButton" />
    <String fx:value="eek" />
  </styleClass>
</Button>
</children>
</GridPane>
